{
  "swagger": "2.0",
  "info": {
    "title": "My API",
    "description": "API for student and agent registration, authentication, and student information management.",
    "version": "1.0.0"
  },
  "host": "localhost:8080",
  "basePath": "/",
  "schemes": ["http"],
  "paths": {
    "/api/auth/register/student": {
      "post": {
        "description": "Register a new student",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "type": "string",
                  "example": "John",
                  "description": "First name of the student"
                },
                "lastName": {
                  "type": "string",
                  "example": "Doe",
                  "description": "Last name of the student"
                },
                "country": {
                  "type": "string",
                  "example": "USA",
                  "description": "Country of residence"
                },
                "phone": {
                  "type": "object",
                  "properties": {
                    "code": {
                      "type": "string",
                      "example": "+1",
                      "description": "Country code for the phone number"
                    },
                    "number": {
                      "type": "string",
                      "example": "1234567890",
                      "description": "Phone number"
                    }
                  },
                  "required": ["code", "number"]
                },
                "studentType": {
                  "type": "string",
                  "example": "Undergraduate",
                  "description": "Type of student (e.g., Undergraduate, Graduate)"
                },
                "email": {
                  "type": "string",
                  "example": "student@example.com",
                  "description": "Email address of the student"
                },
                "password": {
                  "type": "string",
                  "example": "password123",
                  "description": "Password for the account"
                }
              },
              "required": [
                "firstName",
                "lastName",
                "country",
                "phone",
                "studentType",
                "email",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Student registered successfully"
          },
          "400": {
            "description": "Bad Request - Validation Error"
          },
          "409": {
            "description": "Conflict - Email already in use"
          }
        }
      }
    },
    "/api/auth/register/agent": {
      "post": {
        "description": "Register a new agent",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "Corporate",
                  "description": "Type of agent (e.g., Corporate, Individual)"
                },
                "company": {
                  "type": "object",
                  "properties": {
                    "legalName": {
                      "type": "string",
                      "example": "TechCorp Inc.",
                      "description": "Legal name of the company"
                    },
                    "tradeName": {
                      "type": "string",
                      "example": "TechCorp",
                      "description": "Trade name of the company"
                    }
                  },
                  "required": ["legalName"]
                },
                "address": {
                  "type": "object",
                  "properties": {
                    "country": {
                      "type": "string",
                      "example": "USA",
                      "description": "Country of residence"
                    },
                    "state": {
                      "type": "string",
                      "example": "California",
                      "description": "State of residence"
                    },
                    "city": {
                      "type": "string",
                      "example": "San Francisco",
                      "description": "City of residence"
                    },
                    "zipCode": {
                      "type": "integer",
                      "example": 94103,
                      "description": "ZIP code"
                    },
                    "fullAddress": {
                      "type": "string",
                      "example": "1234 Market St, San Francisco, CA 94103",
                      "description": "Full address"
                    }
                  },
                  "required": [
                    "country",
                    "state",
                    "city",
                    "zipCode",
                    "fullAddress"
                  ]
                },
                "founder": {
                  "type": "object",
                  "properties": {
                    "email": {
                      "type": "string",
                      "example": "founder@techcorp.com",
                      "description": "Email address of the founder"
                    },
                    "phone": {
                      "type": "string",
                      "example": "+1234567890",
                      "description": "Phone number of the founder"
                    }
                  },
                  "required": ["email", "phone"]
                },
                "primaryContact": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "example": "Jane Doe",
                      "description": "Name of the primary contact person"
                    },
                    "email": {
                      "type": "string",
                      "example": "contact@techcorp.com",
                      "description": "Email address of the primary contact person"
                    },
                    "phone": {
                      "type": "string",
                      "example": "+1234567890",
                      "description": "Phone number of the primary contact person"
                    }
                  },
                  "required": ["name", "email", "phone"]
                },
                "password": {
                  "type": "string",
                  "example": "securepassword123",
                  "description": "Password for the account"
                }
              },
              "required": [
                "type",
                "company",
                "address",
                "founder",
                "primaryContact",
                "password"
              ]
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Agent registered successfully"
          },
          "400": {
            "description": "Bad Request - Validation Error"
          },
          "409": {
            "description": "Conflict - Email already in use"
          }
        }
      }
    },
    "/api/auth/login": {
      "post": {
        "description": "Login an existing user",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "example": "student",
                  "description": "Type of user (e.g., student, agent)"
                },
                "email": {
                  "type": "string",
                  "example": "user@example.com",
                  "description": "Email address of the user"
                },
                "password": {
                  "type": "string",
                  "example": "password123",
                  "description": "Password for the account"
                }
              },
              "required": ["type", "email", "password"]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Login successful"
          },
          "400": {
            "description": "Bad Request - Validation Error"
          },
          "401": {
            "description": "Unauthorized - Invalid credentials"
          }
        }
      }
    },
    "/api/auth/logout": {
      "post": {
        "description": "Logout the current user",
        "responses": {
          "200": {
            "description": "User logged out successfully"
          },
          "400": {
            "description": "Bad Request - Validation Error"
          },
          "401": {
            "description": "Unauthorized - Invalid or missing credentials"
          }
        }
      }
    },
    "/api/studentinformation/persional-information": {
      "post": {
        "description": "Submit or update student personal information",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "personalInformation": {
                  "type": "object",
                  "description": "Personal information of the student",
                  "properties": {
                    "title": {
                      "type": "string",
                      "description": "Title of the student",
                      "example": "Mr."
                    },
                    "firstName": {
                      "type": "string",
                      "description": "First name of the student",
                      "example": "John"
                    },
                    "middleName": {
                      "type": "string",
                      "description": "Middle name of the student",
                      "example": "M"
                    },
                    "lastName": {
                      "type": "string",
                      "description": "Last name of the student",
                      "example": "Doe"
                    },
                    "email": {
                      "type": "string",
                      "description": "Email address of the student",
                      "example": "john.doe@example.com"
                    },
                    "gender": {
                      "type": "string",
                      "description": "Gender of the student",
                      "example": "Male"
                    },
                    "phoneNumber": {
                      "type": "string",
                      "description": "Phone number of the student",
                      "example": "+123456789"
                    }
                  },
                  "required": [
                    "firstName",
                    "lastName",
                    "email",
                    "gender",
                    "phoneNumber"
                  ]
                },
                "passportDetails": {
                  "type": "object",
                  "description": "Passport details of the student",
                  "properties": {
                    "passportUpload": {
                      "type": "string",
                      "description": "Uploaded passport file",
                      "example": "passport-file-link"
                    },
                    "countryOfCitizenship": {
                      "type": "string",
                      "description": "Country of citizenship",
                      "example": "USA"
                    },
                    "passportNumber": {
                      "type": "string",
                      "description": "Passport number",
                      "example": "123456789"
                    },
                    "expireDate": {
                      "type": "string",
                      "description": "Passport expiry date",
                      "example": "2030-12-31"
                    }
                  },
                  "required": [
                    "passportUpload",
                    "countryOfCitizenship",
                    "passportNumber",
                    "expireDate"
                  ]
                }
              },
              "required": ["personalInformation", "passportDetails"]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Student information updated successfully"
          },
          "400": {
            "description": "Bad Request - Validation Error"
          },
          "404": {
            "description": "Not Found - Student not found"
          }
        }
      }
    },
    "definitions": {},
    "securityDefinitions": {}
  }
}
